    "express": "^4.17.1",
    "knex": "^0.21.5",
    "md5": "^2.3.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/md5": "^2.2.0",
    "@types/node": "^14.6.0",
    "nodemon": "^2.0.4",
    "ts-md5": "^1.2.7",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
